{
  "retainLines": true,
  "compact": "auto",
  "comments": false,
  "presets": [
    ["env", {
      "targets": {
        "electron": ["1.8.4"]
      }
    }]
  ],
  "plugins": [
    "@babel/plugin-proposal-object-rest-spread",
    "@babel/plugin-syntax-class-properties",
    "syntax-jsx",
    "transform-react-jsx",
    "react-hot-loader/babel"
  ],
  "sourceMaps": true
}
